





import matplotlib.pyplot as plt


x=[1,2,3,4,5]
y=[1,4,9,16,25]
# create a line plot

plt.figure()
plt.title("number and square")
plt.plot(x,y)
plt.xlabel("x-axis")
plt.ylabel("y-axis")

plt.show()


x=[1,2,3,4,5]
y=[1,4,9,16,25]

# create customize line plot
plt.plot(x,y,color="r",linestyle='--',marker='*',markersize=50)

plt.grid()


# create multiple plot

# sample data
x=[1,2,3,4,5]
y=[1,4,9,16,25]
z=[1,8,27,64,125]

plt.figure(figsize=(10,10))
plt.subplot(2,2,1)
plt.plot(x,y)


plt.subplot(2,2,2)
plt.plot(x,z,color="r")

plt.subplot(2,2,3)
plt.plot(z,x,color='b')

plt.subplot(2,2,4)
plt.plot(y,z,color='#00FF00')


# Bar plot
